@namespace Douro
@using Douro.Values
@using Douro.Statements
@classname DouroParser

print<Print> =
	"print" _ expr:expression {
		new Print(expr)
	};

expression = sum

sum<Expression>
	= lhs:number _? "+" _? rhs:sum
		{ new Binary(lhs, rhs, Operator.Add) }
	/ number

number<Expression> =
	digit:("" [0-9]+) {
		new Number(digit)
	};

_ = [ \t]+ ;



